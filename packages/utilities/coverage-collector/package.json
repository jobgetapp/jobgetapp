{
  "name": "@jobgetapp/coverage-collector",
  "version": "1.2.0",
  "description": "A tool to collect code coverage across multiple projects in a mono-repo",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "contributors": [
    "sawyer@sawyerhopkins.com"
  ],
  "files": [
    "src",
    "dist",
    "bin.js"
  ],
  "bin": {
    "@jobgetapp/coverage-collector": "bin.js"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc -p src/tsconfig.json && tsc-alias -p src/tsconfig.json",
    "build:watch": "npm run build",
    "lint": "eslint './src/**/*.ts' './test/**/*.ts'",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jobgetapp/jobgetapp.git"
  },
  "keywords": [
    "nodejs"
  ],
  "author": "sawyer@sawyerhopkins.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jobgetapp/jobgetapp/issues"
  },
  "homepage": "https://github.com/jobgetapp/jobgetapp#readme",
  "dependencies": {
    "@rushstack/rush-sdk": "5.100.2",
    "@rushstack/node-core-library": "3.44.2",
    "istanbul-lib-coverage": "3.2.0",
    "istanbul-lib-report": "3.0.0",
    "istanbul-reports": "3.1.4",
    "istanbul-reporter-html-monorepo": "1.1.3",
    "glob": "7.2.0"
  },
  "devDependencies": {
    "@fluffy-spoon/substitute": "1.208.0",
    "@jest/types": "29.6.3",
    "@jobgetapp/eslint-config": "1.0.1",
    "@jobgetapp/jest-config": "1.1.0",
    "@jobgetapp/typescript-config": "1.0.0",
    "@types/jest": "29.5.4",
    "@types/node": "16.11.7",
    "eslint": "8.48.0",
    "jest": "29.6.4",
    "jest-extended": "4.0.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tsc-alias": "1.8.7",
    "typescript": "5.1.6",
    "@types/istanbul-lib-coverage": "2.0.4",
    "@types/istanbul-lib-report": "3.0.0",
    "@types/istanbul-reports": "3.0.1",
    "@types/glob": "7.2.0"
  }
}
