      "@rushstack/node-core-library": "3.44.2",
      "@rushstack/rush-sdk": "5.100.2"
    },
    "devDependencies": {
      "@fluffy-spoon/substitute": "1.208.0",
      "@jest/types": "29.6.3",
      "@jobgetapp/eslint-config": "1.1.0",
      "@jobgetapp/jest-config": "1.2.0",
      "@jobgetapp/typescript-config": "1.1.0",
      "@types/jest": "29.5.4",
      "@types/node": "16.11.7",
      "eslint": "8.48.0",
      "jest": "29.6.4",
      "jest-extended": "4.0.1",
      "ts-jest": "29.1.1",
