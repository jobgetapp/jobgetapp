{
    "name": "@jobgetapp/rush-enforce-categories-plugin",
    "version": "1.0.0",
    "description": "Utility to ensure rush projects to not leak internal code.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "contributors": [
      "Sawyer Hopkins <sawyer@jobget.com>"
    ],
    "files": [
      "src",
      "dist",
      "schemas",
      "command-line.json",
      "rush-plugin-manifest.json"
